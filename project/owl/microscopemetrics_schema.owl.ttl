@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix microscopemetrics_schema: <https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema> a owl:Ontology ;
    rdfs:label "microscopemetrics-schema" ;
    IAO:0000700 microscopemetrics_schema:MetricsDatasetCollection,
        microscopemetrics_schema:NamedThing ;
    linkml:id "https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema" ;
    linkml:imports "linkml:types" .

linkml:SubsetDefinition a owl:Class .

linkml:TypeDefinition a owl:Class .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

microscopemetrics_schema:MetricsDatasetCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "MetricsDatasetCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom microscopemetrics_schema:MetricsDataset ;
            owl:onProperty microscopemetrics_schema:entries ] ;
    skos:definition "A holder for MetricsDataset objects" .

microscopemetrics_schema:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" .

microscopemetrics_schema:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    rdfs:range microscopemetrics_schema:MetricsDataset .

microscopemetrics_schema:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range microscopemetrics_schema:PersonStatus ;
    skos:definition "living or dead status" .

PATO:0001421 a owl:Class,
        microscopemetrics_schema:PersonStatus ;
    rdfs:label "ALIVE" .

PATO:0001422 a owl:Class,
        microscopemetrics_schema:PersonStatus ;
    rdfs:label "DEAD" .

schema1:birthDate a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:exactMatch schema1:birthDate .

schema1:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable description for a thing" ;
    skos:exactMatch schema1:description .

schema1:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    rdfs:range linkml:String ;
    skos:definition "The main email address of a person" ;
    skos:exactMatch schema1:email .

schema1:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:exactMatch schema1:identifier .

schema1:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "A human-readable name for a thing" ;
    skos:exactMatch schema1:name .

microscopemetrics_schema:MetricsDataset a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "MetricsDataset" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty microscopemetrics_schema:age_in_years ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass microscopemetrics_schema:PersonStatus ;
            owl:onProperty microscopemetrics_schema:vital_status ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema1:birthDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema1:email ],
        microscopemetrics_schema:NamedThing ;
    skos:definition "Represents a MetricsDataset" .

microscopemetrics_schema:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema1:description ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema1:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema1:name ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing .

<https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema/PersonStatus#UNKNOWN> a owl:Class,
        microscopemetrics_schema:PersonStatus ;
    rdfs:label "UNKNOWN" .

linkml:ClassDefinition a owl:Class .

microscopemetrics_schema:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "PersonStatus" ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/MontpellierRessourcesImagerie/microscopemetrics-schema/PersonStatus#UNKNOWN> .

linkml:SlotDefinition a owl:Class .


